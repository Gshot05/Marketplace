{
  "name": "Marketplace Dev",
  "dockerFile": "../Dockerfile",
  "context": "..",
  "appPort": [8080],
  "runArgs": ["--init", "--network=host"],
  "settings": {
    "go.useLanguageServer": true,
    "go.toolsManagement.autoUpdate": true
  },
  "extensions": [
    "golang.go"
  ],
  "remoteUser": "root",
  "postCreateCommand": "docker compose up -d"
}
